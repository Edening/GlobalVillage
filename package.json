{
  "name": "vue-cesium-demo",
  "version": "0.0.0",
  "scripts": {
    "cz": "git-cz",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .vue,.js,.ts,.jsx,.tsx && pretty-quick --check --branch dev ./packages",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm -y"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.1",
    "@turf/turf": "^6.5.0",
    "axios": "^0.24.0",
    "axios-mock-adapter": "^1.20.0",
    "dayjs": "^1.10.7",
    "interactjs": "1.4.14",
    "lodash": "^4.17.21",
    "lowdb": "1.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "qs": "^6.10.2",
    "quasar": "^2.7.7",
    "screenfull": "^6.0.0",
    "uuid": "^8.3.2",
    "vue": "3.2.31",
    "vue-cesium": "^3.0.18",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/cz-commitlint": "^13.1.0",
    "@quasar/vite-plugin": "^1.1.1",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "commitizen": "^4.2.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-vue": "^7.16.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "2.5.0",
    "prettier-eslint": "^13.0.0",
    "pretty-quick": "3.1.2",
    "sass": "1.32.0",
    "terser": "^5.14.2",
    "typescript": "^4.7.4",
    "unplugin-vue-define-options": "^0.10.0",
    "vite": "^3.0.2",
    "vue-tsc": "^0.39.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">= 14.12.1"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*.--write": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
